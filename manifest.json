{
    "manifest_version": 3,
    "name": "Banner Extension",
    "version": "1.0",
    "description": "Adds a banner to a specified webpage",
    "permissions": [
        "storage",
        "alarms",
        "tabs"
    ],
    "background": {
        "service_worker": "sw.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.jpeg",
            "48": "images/icon48.jpeg",
            "128": "images/icon128.jpeg"
        }
    },
    "icons": {
        "16": "images/icon16.jpeg",
        "48": "images/icon48.jpeg",
        "128": "images/icon128.jpeg"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["content.js"]
      }
    ]
}